[user]
	name = Pawe≈Ç Korzeniewski
	email = pawel@korzeniewski.pl

[alias]
    c = "!git commit"
    ca = "!git commit --amend"
    fa = "!git fetch --all"
    br = "!git branch"
    co = "!git checkout"
    cp = "!git cherry-pick"
    d = "!git diff"
    dc = "!git diff --cached"
    s = "!git status"
    ss = "!git status -s"
    l = "!git log --graph --abbrev-commit --pretty=oneline --decorate"

    # My Logs.
    ml = "!git l --author=`git config user.email`"

[core]
	excludesfile = ~/.gitignore
	editor = vim
